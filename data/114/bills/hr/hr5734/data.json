    }
  ], 
  "short_title": "Veterans Transplant Coverage Act of 2016", 
  "sponsor": {
    "bioguide_id": "C001051", 
    "district": "31", 
    "name": "Carter, John R.", 
    "state": "TX", 
    "title": "Rep", 
    "type": "person"
  }, 
  "status": "REFERRED", 
  "status_at": "2016-07-12", 
  "subjects": [
    "Armed forces and national security", 
    "Department of Veterans Affairs", 
    "Health facilities and institutions", 
    "Organ and tissue donation and transplantation", 
    "Surgery and anesthesia", 
