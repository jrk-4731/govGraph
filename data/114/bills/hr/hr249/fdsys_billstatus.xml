    <titles>
      <item>
        <chamberCode />
        <title>To amend the Act of June 18, 1934, to reaffirm the authority of the Secretary of the Interior to take land into trust for Indian tribes, and for other purposes.</title>
        <chamberName />
        <titleType>Official Title as Introduced</titleType>
        <parentTitleType />
      </item>
      <item>
        <chamberCode />
        <title>To amend the Act of June 18, 1934, to reaffirm the authority of the Secretary of the Interior to take land into trust for Indian tribes, and for other purposes.</title>
        <chamberName />
        <titleType>Display Title</titleType>
        <parentTitleType />
      </item>
    </titles>
    <amendments />
