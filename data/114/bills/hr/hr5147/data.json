  "introduced_at": "2016-04-29", 
  "number": "5147", 
  "official_title": "To amend title 40, United States Code, to require restrooms in public buildings to be equipped with baby changing facilities.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": "BABIES Act", 
  "sponsor": {
    "bioguide_id": "C001084", 
    "district": "1", 
    "name": "Cicilline, David N.", 
    "state": "RI", 
    "title": "Rep", 
    "type": "person"
  }, 
  "status": "ENACTED:SIGNED", 
  "status_at": "2016-10-07", 
  "subjects": [
