    <summaries>
      <billSummaries>
        <item>
          <versionCode>00</versionCode>
          <actionDesc>Introduced in House</actionDesc>
          <actionDate>2015-01-14</actionDate>
          <text><![CDATA[<p>Amends the Federal Food, Drug, and Cosmetic Act to prohibit the sale of food that contains genetically-engineered fish unless the food bears a label stating that it contains genetically-engineered fish.</p>]]></text>
          <lastSummaryUpdateDate>2015-01-30T19:59:41Z</lastSummaryUpdateDate>
          <updateDate>2015-01-14T05:00:00Z</updateDate>
