  "short_title": "Reducing Barriers for Veterans Education Act of 2015", 
  "sponsor": {
    "bioguide_id": "M001191", 
    "district": "18", 
    "name": "Murphy, Patrick", 
    "state": "FL", 
    "title": "Rep", 
    "type": "person"
  }, 
  "status": "REFERRED", 
  "status_at": "2015-01-21", 
  "subjects": [
    "Armed forces and national security", 
    "Higher education", 
    "Student aid and college costs", 
    "Veterans' education, employment, rehabilitation"
  ], 
  "subjects_top_term": "Armed forces and national security", 
  "summary": {
    "as": "Introduced in House", 
    "date": "2015-01-21T05:00:00Z", 
    "text": "Reducing Barriers for Veterans Education Act of 2015\n\nMakes an individual who is eligible for educational assistance under the Post-9/11 Educational Assistance program eligible for assistance with the fees for applying to institutions of higher education (IHEs).\n\nSets the amount of such assistance payable to an individual at the lesser of: (1) the total application fees charged to the individual by the IHEs, or (2) $750."
  }, 
