    <title>Emergency Forest Restoration Act</title>
    <updateDate>2019-02-13T17:31:23Z</updateDate>
    <notes />
    <latestAction>
      <text>Referred to the Subcommittee on Conservation and Forestry.</text>
      <actionDate>2016-08-30</actionDate>
      <links />
