  "short_title": "State Marriage Defense Act of 2015", 
  "sponsor": {
    "bioguide_id": "W000814", 
    "district": "14", 
    "name": "Weber, Randy K., Sr.", 
    "state": "TX", 
    "title": "Rep", 
