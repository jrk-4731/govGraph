  "introduced_at": "2016-09-08", 
  "number": "853", 
  "official_title": "Authorizing the Speaker of the House of Representatives to initiate or intervene in a civil action regarding the compliance of the executive branch with the provision of law prohibiting relinquishment of the responsibility of the National Telecommunications and Information Administration with respect to Internet domain name system functions.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
  "sponsor": {
    "bioguide_id": "K000376", 
