      <billSummaries>
        <item>
          <actionDate>2019-01-30</actionDate>
          <actionDesc>Introduced in House</actionDesc>
          <versionCode>00</versionCode>
          <text><![CDATA[<p><b>Stop Penalizing Working Seniors Act</b></p> <p>This bill allows Medicare-eligible individuals who are age 65 or older to contribute to health savings accounts if their entitlement to Medicare benefits is limited to hospital insurance benefits under Medicare Part A.</p>]]></text>
          <updateDate>2019-01-30T05:00:00Z</updateDate>
          <name>Introduced in House</name>
          <lastSummaryUpdateDate>2019-02-21T19:59:16Z</lastSummaryUpdateDate>
        </item>
      </billSummaries>
