    <constitutionalAuthorityStatementText />
    <title>Expressing support for the designation of September 2017 as "National Kinship Care Month".</title>
    <createDate>2017-09-22T03:11:18Z</createDate>
    <summaries>
      <billSummaries>
        <item>
          <lastSummaryUpdateDate>2017-10-05T17:45:23Z</lastSummaryUpdateDate>
          <actionDate>2017-09-21</actionDate>
          <name>Introduced in House</name>
          <actionDesc>Introduced in House</actionDesc>
          <text><![CDATA[<p>Expresses support for&nbsp;the designation of National Kinship Care Month. Honors the commitment and dedication of kinship caregivers and advocates. Encourages Congress to implement policies to improve the lives of vulnerable children and families.</p>]]></text>
          <updateDate>2017-09-21T04:00:00Z</updateDate>
          <versionCode>00</versionCode>
        </item>
      </billSummaries>
    </summaries>
