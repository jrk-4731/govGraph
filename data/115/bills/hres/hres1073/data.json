  "introduced_at": "2018-09-20", 
  "number": "1073", 
  "official_title": "Expressing the sense of the House of Representatives that the Presidential Determination of the annual refugee admissions limit in fiscal year 2019 shall be no less than 110,000 and that President Trump and his administration must operate the United States Refugee Admissions Program (USRAP) in good faith.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
  "sponsor": {
    "bioguide_id": "J000288", 
    "district": "4", 
    "name": "Johnson, Henry C. \"Hank,\" Jr.", 
    "state": "GA", 
    "title": "Rep", 
    "type": "person"
  }, 
  "status": "REFERRED", 
  "status_at": "2018-09-20", 
  "subjects": [
    "Congressional oversight", 
    "Immigration", 
    "Immigration status and procedures", 
    "Presidents and presidential powers, Vice Presidents", 
    "Refugees, asylum, displaced persons"
  ], 
  "subjects_top_term": "Immigration", 
  "summary": {
    "as": "Introduced in House", 
    "date": "2018-09-20T04:00:00Z", 
    "text": "Reaffirms the bipartisan commitment of the United States to promote the safety, health, and well-being of refugees, including through U.S. resettlement for those who cannot return home.\n\nUrges President Trump and his administration to affirm the importance of the U.S. Refugee Admissions Program and to make a presidential determination for FY2019 that the admission of at least 110,000 refugees is justified. (Under the program, such determination establishes the overall admissions levels and regional allocations of all refugees for an upcoming fiscal year.)"
  }, 
