      <billSummaries>
        <item>
          <lastSummaryUpdateDate>2018-03-26T16:19:49Z</lastSummaryUpdateDate>
          <name>Introduced in House</name>
          <updateDate>2018-03-08T05:00:00Z</updateDate>
