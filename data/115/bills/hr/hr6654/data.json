  "introduced_at": "2018-08-03", 
  "number": "6654", 
  "official_title": "To prohibit the transfer of a firearm at a gun show by a person who is not a licensed dealer.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
