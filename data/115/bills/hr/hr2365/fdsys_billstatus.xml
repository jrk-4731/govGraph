      <billSummaries>
        <item>
          <lastSummaryUpdateDate>2018-02-06T21:04:02Z</lastSummaryUpdateDate>
          <name>Introduced in House</name>
          <updateDate>2017-05-04T04:00:00Z</updateDate>
