      <billSummaries>
        <item>
          <lastSummaryUpdateDate>2019-03-27T20:27:08Z</lastSummaryUpdateDate>
          <name>Introduced in House</name>
          <updateDate>2017-07-24T04:00:00Z</updateDate>
