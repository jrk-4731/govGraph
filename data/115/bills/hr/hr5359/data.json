  "short_title": "2020 Census IDEA Act", 
  "sponsor": {
    "bioguide_id": "M000087", 
    "district": "12", 
    "name": "Maloney, Carolyn B.", 
    "state": "NY", 
    "title": "Rep", 
    "type": "person"
