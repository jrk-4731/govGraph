      <billSummaries>
        <item>
          <lastSummaryUpdateDate>2017-09-08T18:36:14Z</lastSummaryUpdateDate>
          <name>Introduced in House</name>
          <updateDate>2017-05-16T04:00:00Z</updateDate>
