      <billSummaries>
        <item>
          <lastSummaryUpdateDate>2017-01-11T19:45:40Z</lastSummaryUpdateDate>
          <name>Introduced in House</name>
          <updateDate>2017-01-03T05:00:00Z</updateDate>
