      <billSummaries>
        <item>
          <name>Introduced in Senate</name>
          <actionDesc>Introduced in Senate</actionDesc>
          <versionCode>00</versionCode>
          <lastSummaryUpdateDate>2017-02-27T22:50:25Z</lastSummaryUpdateDate>
