  "introduced_at": "2013-12-11", 
  "number": "3707", 
  "official_title": "To ensure the emergency protection of Iranian dissidents living in Camp Liberty/Hurriya and to provide for their admission as refugees to the United States.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
  "sponsor": {
    "bioguide_id": "R000409", 
    "district": "48", 
