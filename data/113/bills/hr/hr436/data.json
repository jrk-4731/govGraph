  "short_title": "Government Neutrality in Contracting Act", 
  "sponsor": {
    "bioguide_id": "H001052", 
    "district": "1", 
    "name": "Harris, Andy", 
    "state": "MD", 
    "title": "Rep", 
    "type": "person"
  }, 
  "status": "REFERRED", 
  "status_at": "2013-01-29", 
  "subjects": [
