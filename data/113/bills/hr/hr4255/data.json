  "introduced_at": "2014-03-14", 
  "number": "4255", 
  "official_title": "To require the Federal Housing Finance Agency to establish a 6-month moratorium on foreclosure of mortgages guaranteed by Fannie Mae or Freddie Mac on homes of individuals who have lost Federal unemployment insurance as a result of the expiration of such program, and for other purposes.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": "Stop Foreclosures Due to Congressional Dysfunction Act of 2014", 
  "sponsor": {
    "bioguide_id": "C001090", 
    "district": "17", 
    "name": "Cartwright, Matt", 
    "state": "PA", 
    "title": "Rep", 
    "type": "person"
  }, 
  "status": "REFERRED", 
  "status_at": "2014-03-14", 
  "subjects": [
    "Government corporations and government-sponsored enterprises", 
