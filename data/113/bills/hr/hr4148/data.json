  "number": "4148", 
  "official_title": "To phase out cosmetic animal testing and the sale of cosmetics tested on animals.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": "Humane Cosmetics Act", 
  "sponsor": {
    "bioguide_id": "M000933", 
