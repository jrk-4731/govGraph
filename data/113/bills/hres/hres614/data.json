  "introduced_at": "2014-06-10", 
  "number": "614", 
  "official_title": "Strongly supporting the quality and value of diversity and innovation in the Nation's higher education institutions, and strongly disagreeing with the President's proposal to create and administer a Postsecondary Institution Ratings System.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
  "sponsor": {
    "bioguide_id": "G000289", 
    "district": "6", 
    "name": "Goodlatte, Bob", 
